\{{#each this}}
  <div class="analytics-row">
    <div class="analytics-row-name">
      <p>\{{#if userEmail}}\{{userEmail}}\{{else}}\{{ _userEmail }}\{{/if}}</p>
    </div>
    <div class="analytics-row-score">
      <p>\{{#if sessionsCount}}\{{sessionsCount}}\{{else}}\{{ count }}\{{/if}}</p>
    </div>
  </div>
\{{/each}}